@model Trader.Models.FileImportModels.ImportModel
@*
    For more information on enabling MVC for empty projects, visit https://go.microsoft.com/fwlink/?LinkID=397860
*@
@{
}
<h1>Import</h1>
<br />
<form method="post" enctype="multipart/form-data" asp-controller="TradeImport" asp-action="Import">
    <div class="row">
        <label asp-for="selInstrument" class="control-label col-sm-2"></label>
        <div class="col-md-10">
            <select asp-for="selInstrument" asp-items="@Model.InstrumentList"></select>
        </div>
    </div>
	<div class="row">
        <label asp-for="selExchange" class="control-label col-sm-2"></label>
        <div class="col-md-10">
            <select asp-for="selExchange" asp-items="@Model.ExchangeList"></select>
        </div>
    </div>
	<div class="row">
		<label class="control-label col-sm-2">Upload a CSV: </label>
        <div class="col-md-10">
            
            <input type="file" name="file" />
        </div>
    </div>
	<br/ >
    <div class="row">
        <div class="col-md-10">
            <button type="submit" value="Upload" class="btn btn-default">Upload</button>
        </div>
    </div>
</form>